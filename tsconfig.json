{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "outDir": "./dist",
    "strict": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  },
  //  "include": ["src/**/*", "__tests__/helpers"],
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    //    "__tests__"
  ]
}
